@using WebNetCore.Models
@{
    Layout = null;
}
@model IEnumerable<Student>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<ul>
	
	@foreach (var stu in Model)
	{
		<li>@stu.Id : @stu.Name</li>
	}
</ul>
</body>
</html>
